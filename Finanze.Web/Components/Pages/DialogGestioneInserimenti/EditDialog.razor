@using Finanze.Web.Components.Pages.GestioneTransazioni
<MudDialog>
    <DialogContent>
        <GestioneTransazioneContent Trapasso="false" OnEndInsert="EndInsert" IdTransazione="IdTransazione" />
    </DialogContent>
</MudDialog>
@code {
    [CascadingParameter]
    private IMudDialogInstance MudDialog { get; set; }

    [Parameter]
    public int IdTransazione { get; set; }

    private void EndInsert(int i) => MudDialog.Close(DialogResult.Ok(true));

}